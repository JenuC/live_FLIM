   "source": [
    "# Compare spatial vs phasor selections\n",
    "print(\"\\n=== Comparison: Spatial vs Phasor Selection ===\")\n",
    "print(\"\\nSpatial selections are based on pixel position in the image.\")\n",
    "print(\"Phasor selections are based on lifetime characteristics.\\n\")\n",
    "\n",
    "print(\"Spatial Selection Results:\")\n",
    "print(f\"  Region 1: τ = {result1['lma_tau']:.3f} ns ({result1['pixel_count']} pixels)\")\n",
