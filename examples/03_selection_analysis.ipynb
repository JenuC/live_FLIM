   "source": [
    "## Spatial Selection Analysis\n",
    "\n",
    "Create spatial masks to select specific regions and analyze them:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def create_circular_mask(shape, center, radius):\n",
    "    \"\"\"Create a circular mask.\"\"\"\n",
    "    h, w = shape\n",
    "    y, x = np.ogrid[:h, :w]\n",
    "    cy, cx = center\n",
    "    mask = (x - cx)**2 + (y - cy)**2 <= radius**2\n",
    "    return mask\n",
    "\n",
    "def create_rectangular_mask(shape, top_left, bottom_right):\n",
    "    \"\"\"Create a rectangular mask.\"\"\"\n",
    "    h, w = shape\n",
    "    mask = np.zeros((h, w), dtype=bool)\n",
    "    y1, x1 = top_left\n",
    "    y2, x2 = bottom_right\n",
    "    mask[y1:y2, x1:x2] = True\n",
    "    return mask\n",
    "\n",
    "# Create three spatial selections\n",
    "mask1 = create_rectangular_mask((height, width), (10, 5), (54, 27))  # Left region\n",
    "mask2 = create_circular_mask((height, width), (32, 48), 15)  # Center region\n",
    "mask3 = create_rectangular_mask((height, width), (10, 69), (54, 91))  # Right region\n",
    "\n",
    "print(f\"Mask 1: {np.sum(mask1)} pixels\")\n",
    "print(f\"Mask 2: {np.sum(mask2)} pixels\")\n",
    "print(f\"Mask 3: {np.sum(mask3)} pixels\")"
   ]
  },
