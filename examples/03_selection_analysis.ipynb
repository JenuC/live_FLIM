    "## Compute Phasor and Lifetime Maps"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Configure FLIM parameters\n",
    "flim_params = FlimParams(period=period, fit_start=10, fit_end=200)\n",
    "\n",
    "# Compute phasor\n",
    "phasor = PhasorComputer.compute_phasor(photon_count, flim_params)\n",
    "\n",
    "# Compute lifetime\n",
    "rld_result = FittingEngine.compute_rld(photon_count, flim_params)\n",
    "lma_result = FittingEngine.compute_lma(photon_count, flim_params)\n",
    "\n",
    "# Compute intensity\n",
    "intensity = np.sum(photon_count, axis=-1)\n",
    "\n",
    "print(\"Computed phasor and lifetime maps\")"
   ]
